<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

  <CheckBoxPreference
    app:defaultValue="@bool/pref_main_deep_search_default"
    app:iconSpaceReserved="false"
    app:key="@string/pref_main_deep_search_key"
    app:singleLineTitle="false"
    app:summary="@string/pref_main_deep_search_summary"
    app:title="@string/pref_main_deep_search_title" />

  <CheckBoxPreference
    app:defaultValue="@bool/pref_main_case_sensitive_search_default"
    app:iconSpaceReserved="false"
    app:key="@string/pref_main_case_sensitive_search_key"
    app:singleLineTitle="false"
    app:summary="@string/pref_main_case_sensitive_search_summary"
    app:title="@string/pref_main_case_sensitive_search_title" />

  <CheckBoxPreference
    app:defaultValue="@bool/pref_settings_special_search_default"
    app:iconSpaceReserved="false"
    app:key="@string/pref_settings_special_search_key"
    app:singleLineTitle="false"
    app:summary="@string/pref_settings_special_search_summary"
    app:title="@string/pref_settings_special_search_title" />

  <com.mirfatif.permissionmanagerx.prefs.settings.NumPickerPrefFlavor
    app:defaultValue="@integer/pref_settings_search_suggestions_count_default"
    app:enabled="false"
    app:iconSpaceReserved="false"
    app:key="@string/pref_settings_search_suggestions_count_key"
    app:singleLineTitle="false"
    app:summary="@string/pref_settings_search_suggestions_count_summary" />

  <Preference
    app:enabled="false"
    app:iconSpaceReserved="false"
    app:key="@string/pref_settings_search_keywords_key"
    app:summary="@string/pref_settings_search_keywords_summary"
    app:title="@string/pref_settings_search_keywords_title" />

</PreferenceScreen>
